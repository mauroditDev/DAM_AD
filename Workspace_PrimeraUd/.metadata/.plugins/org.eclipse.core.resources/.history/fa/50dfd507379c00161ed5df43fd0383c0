package ud2p1;

import java.io.Serializable;

public class PersonaSegundo implements Serializable{
	
	int	edad; // >0
	String nombre;
	char sexo; //M - Masculino, F - Femenino
	
	public PersonaSegundo(int	edad, String nombre, char sexo) {
		this.edad = edad;
		this.nombre	= nombre;
		this.sexo = sexo;
	}
	
	public String toString() {
		return "Persona [edad="	+ edad + ", nombre=" + nombre + ", sexo=" + sexo + "]";
	}
	
}
