package practica1;

import java.util.ArrayList;

public class Cuenta {
	
	public Cuenta(ArrayList<Integer> titulares, int id, int saldo){
		this.titulares = titulares;
		this.id = id;
		this.saldo = saldo;
	}
	
	public Cuenta(){
		titulares = new ArrayList<>();
		id = saldo = 0;
	}
	
	public ArrayList<Integer> titulares;
	public int id;
	public int saldo;

}
