package ud2p1;

import java.io.BufferedReader;
import java.io.File;
import java.io.FileReader;

public class ImprimirCsv {

	public static void main(String[] args) {
		try {
			BufferedReader br = new BufferedReader(new FileReader(new File("origen.csv")));
			String texto = "";
			while(true){
				String a;
				a = br.readLine();
				if(a==null)
					break;
				else{
					String[] b = a.split(";");
					for(int i =0; i<b.length; i++){
						texto += b[i] + '\t';
					}
					texto += '\n';
				}
			}
			 System.out.println(texto);
			
		} catch (Exception e) {
			
			e.printStackTrace();
			
		}

	}

}
