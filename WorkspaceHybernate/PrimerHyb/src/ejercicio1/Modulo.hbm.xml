<?xml version="1.0" encoding="UTF-8"?>
 <!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
 <hibernate-mapping>
   <class name="ejemplo09.Modulo" >
     <id column="IdModulo" name="idModulo" type="integer"/>
     <property name="nombre" />
     <property name="patatas" />
      <set name="profesores" table="prof_mod"  cascade="all" inverse="false"  >
          <key>
             <column name="id_mod"  />
         </key>
         <many-to-many column="id_prof" class="ejercicio1.Profesor" />
     </set>
   </class>
 </hibernate-mapping>